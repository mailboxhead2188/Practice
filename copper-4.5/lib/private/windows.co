//[cf]
//[of]:Functions
function MAKEWORD (a, b)
	return (a:BYTE:WORD) | (b:BYTE:WORD << 8)
end

function MAKELONG (a: WORD, b: WORD)
	return ( (a:WORD:DWORD) | (b:WORD:DWORD) << 16)  : LONG
end

function LOWORD (l: DWORD)
	return l	: WORD
end

function HIWORD (l: DWORD)
	return ((l >> 16) & 0xFFFF) : WORD
end

function LOBYTE (x)
	return x	: BYTE
end

function HIBYTE (w: WORD)
	return (w >> 8) : BYTE & 0xFF
end
