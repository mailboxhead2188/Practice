struct GUID
    var Data1	: DWORD
    var Data2	: WORD
    var Data3	: WORD
    var Data4	: [8] BYTE
end

struct RECT
	var left	: Int32
	var top	: Int32
	var right	: Int32
	var bottom	: Int32

	function width
		return self right - self left
	end
	
	function height
		return self bottom - self top
	end
end

